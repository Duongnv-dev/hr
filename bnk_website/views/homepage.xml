<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <template id="portal_show_sign_in" inherit_id="portal.frontend_layout">
        <xpath expr="//a[@t-attf-href='/web/login']" position="replace"></xpath>
<!--        <xpath expr="//li[2]" position="replace">-->
